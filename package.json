{
  "name": "karlquerel.github.io",
  "version": "1.0.0",
  "description": "You can access the website [here](karlquerel.github.io)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cp dist/index.html dist/404.html",
    "serve": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "vite build && cp dist/index.html dist/404.html && gh-pages -d dist",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss}\" --config .prettierrc && npm run lint:fix && npm run format:check && npm run lint:check",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,scss}\" --config .prettierrc",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "lint:check": "eslint . --ext .js,.jsx --max-warnings 0",
    "clean": "npm run lint:fix && npm run format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "sass": "^1.85.0",
    "terser": "^5.43.1",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "esbuild": "^0.25.0",
    "firebase": "^11.3.1",
    "nes.css": "^2.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "typeit": "^8.8.7"
  }
}
