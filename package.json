{
	"name": "karlquerel.github.io",
	"version": "1.0.0",
	"description": "You can access the website [here](karlquerel.github.io)",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"postbuild": "cp dist/index.html dist/404.html",
		"serve": "vite preview",
		"test": "echo \"Error: no test specified\" && exit 1",
		"deploy": "vite build && cp dist/index.html dist/404.html && gh-pages -d dist",
		"format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"*.{js,json}\" --config .prettierrc",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" \"*.{js,json}\" --config .prettierrc",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
		"fix": "npm run format && npm run lint:fix",
		"fix:long-lines": "node scripts/fix-long-classnames.mjs",
		"fix:prettier-ignore": "node scripts/add-prettier-ignore.mjs",
		"type-check": "tsc --noEmit",
		"clean": "npm run lint:fix && npm run format"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@vitejs/plugin-react": "^5.1.1",
		"autoprefixer": "^10.4.22",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.37.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"gh-pages": "^6.3.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"tailwindcss": "^3.4.18",
		"terser": "^5.43.1",
		"typescript": "^5.9.3",
		"vite": "^6.3.4"
	},
	"dependencies": {
		"esbuild": "^0.25.0",
		"firebase": "^11.3.1",
		"nes.css": "^2.2.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router-dom": "^7.9.6",
		"typeit": "^8.8.7"
	}
}
